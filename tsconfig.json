{
  "compilerOptions": {
    "target": "es5",
    "module": "ES6",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "lib": [
      "ESNext"
    ]
  },
  "include": [
    "lib"
  ]
}
